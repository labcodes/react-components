{
  "name": "react-ui-components",
  "version": "1.0.0",
  "description": "A collection of React components, ready to use!",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel sass:watch storybook:dev",
    "build": "npm-run-all --parallel sass:build storybook:build",
    "storybook:dev": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .components",
    "storybook:gh": "storybook-to-ghpages",
    "sass:build": "node-sass-chokidar src/ -o src/",
    "sass:watch": "npm run sass:build -- --watch --recursive",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "live": "./node_modules/.bin/http-server .components -o",
    "demo": "npm run build && npm run live",
    "deploy": "npm run build && npm run storybook:gh"
  },
  "author": "Thulio Philipe <thulio@labcodes.com.br>",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-info": "^3.4.3",
    "@storybook/addon-knobs": "^3.4.3",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-notes": "^3.3.15",
    "@storybook/addon-options": "^3.4.3",
    "@storybook/addon-storyshots": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "http-server": "^0.11.1",
    "jest": "^22.4.3",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.3.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3"
  },
  "dependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
